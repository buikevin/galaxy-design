{
  "buildCommand": "bun run build",
  "outputDirectory": ".vitepress/dist",
  "rootDirectory": "docs",
  "framework": "vitepress",
  "installCommand": "bun install",
  "devCommand": "bun run dev",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
